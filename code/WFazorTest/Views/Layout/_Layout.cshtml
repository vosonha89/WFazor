    <meta http-equiv="X-UA-Compatible" content="IE=11">
    <!-- Bootstrap core CSS -->
    <link href="@Html.Content("Content/css/bootstrap.min.css")" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="@Html.Content("Content/css/pricing.css")" rel="stylesheet" />

    <!-- Placed at the end of the document so the pages load faster -->
    <script src="@Html.Content("Content/js/jquery-3.3.1.slim.min.js")"></script>
